{
  "name" : "lzerma/gearman-bundle-forked",
  "description" : "Adds gearman support to your Symfony2 project",
  "version" : "1.0",
  "require" : {
    "php" : ">=5.3.3",
    "symfony/framework-bundle" : ">=2.2",
    "doctrine/common" : ">=2.2",
    "symfony/console" : ">=2.1",
    "symfony/config" : ">=2.1",
    "symfony/http-kernel" : ">=2.1",
    "symfony/dependency-injection" : ">=2.1",
    "doctrine/doctrine-cache-bundle" : "1.0.0",
    "symfony/finder" : ">=2.1"
  },
  "replace" : {
    "mmoreramerino/gearman-bundle" : "dev-master"
  },
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*"
  },
  "target-dir" : "Mmoreram/GearmanBundle",
  "authors" : [ {
    "name" : "Marc Morera",
    "email" : "yuhu@mmoreram.com"
  }, {
    "name" : "Symfony Community",
    "homepage" : "https://github.com/mmoreram/GearmanBundle/contributors"
  } ],
  "keywords" : [ "gearman" ],
  "minimum-stability" : "stable",
  "extra" : {
    "branch-alias" : {
      "dev-master" : "2.3-dev"
    }
  },
  "autoload" : {
    "psr-0" : {
      "Mmoreram\\GearmanBundle" : ""
    }
  },
  "license" : "MIT",
  "type" : "symfony-bundle",
  "homepage" : "https://github.com/mmoreram/GearmanBundle"
}